{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kousik-data-factory"
		},
		"ls_azuresql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azuresql'"
		},
		"ls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake'"
		},
		"ls_omprem_file_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_omprem_file'"
		},
		"ls_azuresql_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "adfprojectkousik.database.windows.net"
		},
		"ls_azuresql_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfprojectdb"
		},
		"ls_azuresql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminadmin"
		},
		"ls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfkousikstorage.dfs.core.windows.net/"
		},
		"ls_github_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"ls_omprem_file_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\ADF_Project"
		},
		"ls_omprem_file_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "KousikPC\\kousikADF"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kousik-data-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_azuresql_password": {
						"value": "[parameters('ls_azuresql_password')]"
					},
					"ls_datalake_accountKey": {
						"value": "[parameters('ls_datalake_accountKey')]"
					},
					"ls_omprem_file_password": {
						"value": "[parameters('ls_omprem_file_password')]"
					},
					"ls_azuresql_properties_typeProperties_server": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_server')]"
					},
					"ls_azuresql_properties_typeProperties_database": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_database')]"
					},
					"ls_azuresql_properties_typeProperties_userName": {
						"value": "[parameters('ls_azuresql_properties_typeProperties_userName')]"
					},
					"ls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_properties_typeProperties_url')]"
					},
					"ls_github_properties_typeProperties_url": {
						"value": "[parameters('ls_github_properties_typeProperties_url')]"
					},
					"ls_omprem_file_properties_typeProperties_host": {
						"value": "[parameters('ls_omprem_file_properties_typeProperties_host')]"
					},
					"ls_omprem_file_properties_typeProperties_userId": {
						"value": "[parameters('ls_omprem_file_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "kousik-data-factory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'kousik-data-factory_ArmTemplate_0')]"
			]
		}
	]
}